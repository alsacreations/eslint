{
  "name": "eslint-config-alsacreations",
  "version": "2.1.1",
  "description": "Configuration eslint d'Alsacréations",
  "packageManager": "pnpm@9.0.6",
  "repository": {
    "type": "git",
    "url": "https://github.com/alsacreations/eslint"
  },
  "exports": {
    "./javascript": {
      "import": "./dist/configs/javascript/index.mjs",
      "types": "./dist/configs/javascript/index.d.ts"
    },
    "./prettier": {
      "import": "./dist/configs/prettier/index.mjs",
      "types": "./dist/configs/prettier/index.d.ts"
    },
    "./typescript": {
      "import": "./dist/configs/typescript/index.mjs",
      "types": "./dist/configs/typescript/index.d.ts"
    },
    "./vue": {
      "import": "./dist/configs/vue/index.mjs",
      "types": "./dist/configs/vue/index.d.ts"
    },
    "./solid": {
      "import": "./dist/configs/solid/index.mjs",
      "types": "./dist/configs/solid/index.d.ts"
    },
    "./solid-typescript": {
      "import": "./dist/configs/solid-typescript/index.mjs",
      "types": "./dist/configs/solid-typescript/index.d.ts"
    }
  },
  "bin": "dist/bin/bin.js",
  "scripts": {
    "build": "tsup",
    "release": "pnpm changelogen --release --push --token $(gh auth token)",
    "bin-exec": "tsx bin/bin.ts init"
  },
  "files": [
    "dist",
    "tsconfig.json"
  ],
  "keywords": [],
  "author": "Alsacréations",
  "license": "ISC",
  "peerDependencies": {
    "eslint": "^8.28.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "@types/common-tags": "^1.8.4",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.12.7",
    "@types/prettier": "^2.7.3",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@typescript-eslint/utils": "^7.8.0",
    "bumpp": "^9.4.1",
    "chalk": "^5.3.0",
    "changelogen": "^0.5.5",
    "commander": "^12.0.0",
    "common-tags": "^1.8.2",
    "consola": "^3.2.3",
    "eslint": "^9.1.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-solid": "^0.14.0",
    "eslint-plugin-vue": "^9.25.0",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
    "find-up": "^7.0.0",
    "local-pkg": "^0.5.0",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "tsx": "^4.7.3",
    "type-fest": "^4.18.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  }
}
